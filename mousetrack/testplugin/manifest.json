{
    "manifest_version": 2,
    "name": "trumptech",
    "version": "1.0",
    "description": "Trumptech data collection tools",
    "browser_action": {},
    "permissions": [
        "http://*/",
        "bookmarks",
        "tabs",
        "history"
    ],
    "content_scripts": [{
        "matches": ["http://vizcourse.hkustvis.org/*"],
        "js": ["index.js"]
    }]
}